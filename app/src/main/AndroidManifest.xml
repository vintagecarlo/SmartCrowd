<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.smartcrowd.patch.smart_crowd">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION"/>
    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:theme="@style/MyMaterialTheme" >
        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/google_maps_key" />
        <activity
            android:name=".LoginActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" >
        </activity>
        <activity android:name=".RegisterActivity" >
        </activity>
        <activity android:name=".ProfileActivity" >
        </activity>
        <activity android:name=".AccountSettings" >
        </activity>
        <activity android:name=".PasswordSettings" >
        </activity>
        <activity android:name=".profileMainDisplay">
        </activity>
        <activity android:name=".AlphaMap">
        </activity>
        <activity android:name=".BetaMap">
        </activity>
        <activity android:name=".Unverified">
        </activity>
        <activity android:name=".LocationUser"></activity>
        <activity android:name=".SearchPost"></activity>
        <activity android:name=".TagPost"></activity>
        <service android:name=".notification">
        </service>
    </application>
</manifest>